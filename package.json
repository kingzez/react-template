{
  "name": "react-template",
  "version": "0.1.0",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "serve": "serve -s build",
    "pipeline:dev": "env-cmd -f .env.develop npm run build",
    "pipeline:test": "env-cmd -f .env.test npm run build",
    "pipeline:staging": "env-cmd -f .env.staging npm run build",
    "pipeline:prod": "env-cmd -f .env.production npm run build"
  },
  "author": "wangzezhi",
  "homepage": "http://kingzez.github.io/react-template",
  "dependencies": {
    "@ant-design/compatible": "^1.0.2",
    "antd": "^4.1.1",
    "axios": "^0.21.1",
    "connected-react-router": "^6.8.0",
    "history": "^4.10.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.3.0",
    "swr": "^0.2.0",
    "typescript": "3.8.3"
  },
  "devDependencies": {
    "@types/jest": "25.2.1",
    "@types/node": "13.11.0",
    "@types/react": "16.9.32",
    "@types/react-dom": "16.9.6",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "babel-plugin-import": "^1.13.0",
    "customize-cra": "^0.9.1",
    "env-cmd": "^10.1.0",
    "husky": "^4.2.3",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.1.2",
    "node-sass": "^4.13.1",
    "prettier": "^2.0.4",
    "react-app-rewired": "^2.1.5",
    "serve": "^11.3.0",
    "source-map-explorer": "^2.4.2",
    "typescript-plugin-css-modules": "^2.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
